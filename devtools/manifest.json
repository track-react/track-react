{
  "name": "track-react",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "DevTools panel that displays messages bridged from window.postMessage.",
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*",
        "https://localhost/*"
      ],
      "js": ["content.js"]
    }
  ]
}
